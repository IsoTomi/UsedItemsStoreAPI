{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example2.json",
  "type": "object",
  "title": "Item info schema",
  "description": "The root schema comprises the entire JSON document.",
  "default": {},
  "examples": [
      {
          "id": 345,
          "title": "Asus Geforce Rtx 3070 8gb oc",
          "description": "Takuu voimassa 05/24. Ei lhr malli.",
          "category": "Näytönohjaimet",
          "askingPrice": 1000.00,
          "dateOfPosting": "2022-02-26",
          "deliveryType": "Pickup",
          "sellerId": "5d06110d-ef59-4734-a32c-190bb911bc88",
          "city": "Oulu",
          "county": "Pohjois-Pohjanmaa",
          "country": "Finland",
          "items": [
              {"id": "37567b4e-f297-491e-ad90-dda73a08f56f"},
              {"id": "56779b4e-f297-491e-ad90-dda73a08f89a"}
          ]
      }
  ],
  "required": [
      "title",
      "description",
      "category",
      "askingPrice",
      "dateOfPosting",
      "deliveryType",
      "sellerId",
      "city",
      "county",
      "country"
  ],
  "properties": {
      "id": {
          "$id": "#/properties/id",
          "type": "integer",
          "title": "The item id schema",
          "description": "Describes item's id property.",
          "default": "Id is unique value used to identify item.",
          "examples": [
              345
          ]
      },
      "title": {
          "$id": "#/properties/title",
          "type": "string",
          "title": "The item title schema",
          "description": "Describes item's title property.",
          "default": "",
          "examples": [
              "Asus Geforce Rtx 3070 8gb oc"
          ]
      },
      "description": {
          "$id": "#/properties/description",
          "type": "string",
          "title": "The item's description schema",
          "description": "Describes item's description property.",
          "default": "",
          "examples": [
              "Takuu voimassa 05/24. Ei lhr malli."
          ]
      },
      "category": {
          "$id": "#/properties/category",
          "type": "string",
          "title": "The item's category schema",
          "description": "Describes item's category property.",
          "default": "",
          "examples": [
              "Näytönohjaimet"
          ]
      },
      "askingPrice": {
          "$id": "#/properties/askingPrice",
          "type": "number",
          "title": "The item's asking price schema",
          "description": "Describes item's askingPrice property.",
          "default": "",
          "examples": [
              1000.00
          ]
      },
      "dateOfPosting": {
          "$id": "#/properties/dateOfPosting",
          "type": "string",
          "title": "The item's date of posting schema",
          "description": "Describes item's dateOfPosting property.",
          "default": "",
          "examples": [
              "2022-02-26"
          ]
      },
      "deliveryType": {
          "$id": "#/properties/deliveryType",
          "type": "string",
          "title": "The item's delivery type schema",
          "description": "Describes item's deliveryType property.",
          "default": "",
          "examples": [
              "Pickup"
          ]
      },
      "sellerId": {
          "$id": "#/properties/userId",
          "type": "string",
          "title": "The item's sellerId schema",
          "description": "Describes item's sellerItem property. sellerId is the userId of the seller.",
          "default": "",
          "examples": [
              "5d06110d-ef59-4734-a32c-190bb911bc88"
          ]
      },
      "city": {
          "$id": "#/properties/city",
          "type": "string",
          "title": "The item's  city schema",
          "description": "Describes item's city property.",
          "default": "",
          "examples": [
              "Oulu"
          ]
      },
      "county": {
          "$id": "#/properties/county",
          "type": "string",
          "title": "The item's county schema",
          "description": "Describes item's county property.",
          "default": "",
          "examples": [
              "Pohjois-Pohjanmaa"
          ]
      },
      "country": {
          "$id": "#/properties/country",
          "type": "string",
          "title": "The item's country schema",
          "description": "Describes item's country property.",
          "default": "",
          "examples": [
              "Finland"
          ]
      }
  },
  "additionalProperties": true
}